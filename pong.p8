pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- magic numbers
width = 128
half = width/2
padding = 10
paddle_speed = 3
offset_power = 2.5
buttons = {
  left = 0,
  right = 1,
  up = 2,
  down = 3,
  a = 4,
  b = 5
}
-- state
player = {
  x = padding,
  y = half,
  left_facing = false,
  score = 0,
  speed = paddle_speed
}
npc = {
  x = width-padding,
  y = half,
  left_facing = true,
  score = 0,
  speed = 1
}
ball = {
  x = half,
  y = half,
  speed = 2,
  dirx = 1,
  diry = 0,
  radius = 4
}

-- acting

function move_player()
  if button("up") then
    player.y -= paddle_speed
  elseif button("down") then
    player.y += paddle_speed
  end
  bound(player)
end

function move_npc()
  distance = ball.y - npc.y
  if distance > -npc.speed
  and distance < npc.speed then
    npc.y = ball.y
  elseif distance > 0 then
    npc.y += npc.speed
  elseif distance < 0 then
    npc.y -= npc.speed
  end
  bound(npc)
end

function bound(paddle)
  if paddle.y-8 < 0 then
    paddle.y = 8
  elseif paddle.y+8 > width then
    paddle.y = width-8
  end
end

function move_ball()
  ball.x += ball.dirx * ball.speed
  ball.y += ball.diry * ball.speed
  edge_collision()
  score_collision()
  if not paddle_collision() then return end
  ball.x = calculate_bounce()
end

function edge_collision()
  offset = 0
  if ball.y-ball.radius < 0 then
    offset = ball.radius
  elseif ball.y+ball.radius > width then
    offset = -ball.radius
  end
  if offset != 0 then
    ball.y += offset
    ball.diry *= -1
  end
end

function calculate_bounce()
  -- todo: refactor
  paddle = player
  new_x = ball.x+ball.radius
  vector_x = 8/offset_power
  if ball.x > half then
    paddle = npc
    new_x = ball.x-ball.radius
    vector_x *= -1
  end
  vector_y = ball.y-paddle.y
  length = vector_length(vector_x,vector_y)
  ball.dirx = vector_x/length
  ball.diry = vector_y/length
  return new_x
end

function paddle_collision()
  return ball_hitting(player)
      or ball_hitting(npc)
end

function score_collision()
  scored = false
  if ball.x > width then
    scored = true
    player.score += 1
  elseif ball.x < 0 then
    scored = true
    npc.score += 1
  end
  if scored then reset_ball() end
end

function reset_ball()
  ball.x = half
  ball.y = half
  ball.dirx = 1
  ball.diry = 0
end

function ball_hitting(paddle)
  return ball.x-ball.radius < paddle.x
     and ball.x+ball.radius > paddle.x
     and ball.y <= paddle.y+8
     and ball.y >= paddle.y-8
end

function _update()
  move_player()
  move_npc()
  move_ball()
end

-- drawing  

function _draw()
  cls()
  line(half,0,half,width)
  draw_paddle(player)
  draw_paddle(npc)
  draw_ball()
  draw_score()
end

function draw_score()
  print(player.score,5,5)
  print(npc.score,half+5,5)
end

function draw_paddle(actor)
  drawx = actor.x
  if not actor.left_facing then
    drawx = actor.x-8
  end
  spr(1,drawx,actor.y-8,1,2,
      actor.left_facing,false)
end

function draw_ball()
  spr(0,ball.x-4,ball.y-4)
end

-- helpers

function vector_length(x,y)
  return sqrt((x*x)+(y*y))
end

function button(name)
  return btn(buttons[name])
end

