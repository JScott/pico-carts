pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- magic numbers
width = 128
half = width/2
padding = 10
paddle_speed = 3
buttons = {
  left = 0,
  right = 1,
  up = 2,
  down = 3,
  a = 4,
  b = 5
}
-- state
player = {
  x = padding-8,
  y = half,
  left_facing = false,
  behind = padding
}
npc = {
  x = width-padding,
  y = half,
  left_facing = true,
  behind = width-padding
}
ball = {
  x = half,
  y = half,
  speed = 3,
  dirx = 1,
  diry = 0
}

-- acting

function move_player()
  if button("up") then
    player.y -= paddle_speed
  elseif button("down") then
    player.y += paddle_speed
  end
end

function move_ball()
  ball.x += ball.dirx * ball.speed
  ball.y += ball.diry * ball.speed
  if not collision(ball) then return end
  ball.x = calculate_bounce(ball)
end

function calculate_bounce(ball)
  ball.dirx = -1
  if ball.x > half then
    return npc.behind-4
  else
    return player.behind+4
  end
end

function collision(ball)
  hit_player = ball_hitting(player) and
    ball.x-4 < player.behind
  hit_npc = ball_hitting(npc) and
    ball.x+4 > npc.behind
  return hit_player or hit_npc
end

function ball_hitting(paddle)
  return ball.y <= paddle.y+8
     and ball.y >= paddle.y-8
end

function _update()
  move_player()
  move_ball()
end

-- drawing  

function _draw()
  cls()
  draw_paddle(player)
  line(half,0,half,width)
  draw_paddle(npc)
  draw_ball()
end

function draw_paddle(actor)
  spr(1,actor.x,actor.y-8,1,2,
      actor.left_facing,false)
end

function draw_ball()
  spr(0,ball.x-4,ball.y-4)
end

-- helpers

function vector_length(x,y)
  return sqrt((x*x)+(y*y))
end

function button(name)
  return btn(buttons[name])
end

